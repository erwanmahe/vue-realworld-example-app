  "description": "TodoMVC for the RealWorldâ„¢",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-plugin-pwa": "^3.10.0",
    "@vue/cli-plugin-unit-jest": "^4.0.5",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.1",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.12.0",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "pretty-quick"
  },
  "pretty-quick": {
    "*.{js,vue}": [
      "npm run pretty-quick -- --staged"
    ]
  },
  "name": "realworld-vue",
  "scripts": {
    "build": "cross-env BABEL_ENV=dev vue-cli-service build",
    "serve": "cross-env BABEL_ENV=dev vue-cli-service serve",
    "test": "cross-env BABEL_ENV=test jest --coverage",
    "lint": "vue-cli-service lint --no-fix {src,tests}/**.{js,vue}",
    "lint:fix": "vue-cli-service lint --fix {src,tests}/**.{js,vue}",
    "prettier": "npm run prettier -- --check {src,tests}/**.{js,vue}",
    "prettier": "npm run prettier -- --write {src,tests}/**.{js,vue}"
  },
  "version": "0.1.0"
}
